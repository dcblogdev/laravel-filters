{
    "name": "daveismyname/laravel-filters",
    "description": "Save and reuse query string based filters",
    "license": "MIT",
    "authors": [
        {
            "name": "David Carr",
            "email": "dave@daveismyname.com",
            "homepage": "https://daveismyname.blog"
        }
    ],
    "homepage": "https://github.com/daveismyname/laravel-filters",
    "keywords": ["Laravel", "Filter"],
    "require": {
        "ext-json": "*",
        "laravel/framework": "^5.5|^5.6|^5.7|^5.8|^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Daveismyname\\Filters\\": "src/",
            "Daveismyname\\Filters\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Daveismyname\\Filters\\FiltersServiceProvider"
            ],
            "aliases": {
                "Filters": "Daveismyname\\Filters\\Facades\\Filters"
            }
        }
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^9.5"
    }
}
