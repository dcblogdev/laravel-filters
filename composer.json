{
    "name": "dcblogdev/laravel-filters",
    "description": "Save and reuse query string based filters",
    "license": "MIT",
    "authors": [
        {
            "name": "David Carr",
            "email": "dave@dcblog.dev",
            "homepage": "https://dcblog.dev"
        }
    ],
    "homepage": "https://github.com/dcblogdev/laravel-filters",
    "keywords": ["Laravel", "Filter"],
    "require": {
        "ext-json": "*",
        "laravel/framework": "^5.5|^5.6|^5.7|^5.8|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Dcblogdev\\Filters\\": "src/",
            "Dcblogdev\\Filters\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dcblogdev\\Filters\\FiltersServiceProvider"
            ],
            "aliases": {
                "Filters": "Dcblogdev\\Filters\\Facades\\Filters"
            }
        }
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0"
    }
}
